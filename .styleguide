cppHeaderFileInclude {
  \.h$
  \.inc$
  \.inl$
}

cppSrcFileInclude {
  \.cpp$
}

generatedFileExclude {
  sysid-application/src/main/native/resources
  sysid-application/src/main/native/win/sysid.ico
  sysid-application/src/main/native/mac/sysid.icns
  gradlew
}

repoRootNameOverride {
  sysid
}

includeOtherLibs {
  ^Eigen/
  ^GLFW/
  ^cscore/
  ^ctre/
  ^fmt/
  ^frc/
  ^glass/
  ^gtest/
  ^imgui
  ^implot\.h$
  ^libssh/
  ^networktables/
  ^portable-file-dialogs
  ^rev/
  ^units/
  ^uv/
  ^wpi/
  ^wpigui
  ^wpimath/
  ^wpinet/
}
